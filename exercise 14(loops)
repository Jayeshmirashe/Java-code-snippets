package com.company;

import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        System.out.print("enter a number ");
        Scanner input=new Scanner(System.in);
        int number=input.nextInt();
        for(int i=1;i<=number;i++){
            for(int j=1;j<=number;j++){
                System.out.print("*");
            }
            System.out.println("");
        }
        }
    }


